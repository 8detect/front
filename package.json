{
  "name": "minds-front",
  "version": "1.0.0",
  "description": "Front end repository for Minds",
  "scripts": {
    "postinstall": "typings install",
    "gulp": "gulp",
    "build": "npm run build-prod && npm run build-index-prod --",
    "build-i18n": "npm run build-i18n-prod && npm run build-index-i18n-prod --",
    "test": "karma start",
    "extract": "gulp prepare.prod && ng-xi18n -p angular-tsconfig.json --i18nFormat xlf",
    "postextract": "rimraf .ngcli-build/ && rmv \"^messages\\.(.*)$\" ./app/locale/Default.\\$1 && gulp lint.xlf --source=Default.xlf --soft",
    "clean": "rimraf .tmp/ .tmp.aot/",
    "compile-aot": "ngc -p tsconfig-aot.json",
    "compile-embed-aot": "ngc -p tsconfig-embed-aot.json",
    "bundle": "rollup -c rollup-config.ts",
    "bundle-embed": "rollup -c rollup-config-embed.ts",
    "prepare-prod": "gulp prepare.prod",
    "build-shims": "gulp build.bundle.prod --env=prod",
    "prebuild-app": "npm run clean",
    "build-app": "npm run compile-aot && npm run bundle -- -o public/js/build-aot.${MINDS_RELEASE:-default}.js",
    "prebuild-embed": "npm run clean",
    "build-embed": "npm run compile-embed-aot && npm run bundle-embed -- -o public/js/build-embed-aot.${MINDS_RELEASE:-default}.js",
    "build-index-prod": "gulp build.index --env=prod --locale-index-file-name=index-aot --v=${MINDS_RELEASE:-default}",
    "prebuild-prod": "npm run clean",
    "build-prod": "npm run prepare-prod && npm run build-shims && npm run build-app && npm run build-embed",
    "postbuild-prod": "npm run clean",
    "build-i18n-prod": "gulp prepare.prod && npm run compile-i18n-prod && npm run bundle-i18n-prod",
    "prebuild-i18n-prod": "npm run clean",
    "postbuild-i18n-prod": "npm run clean",
    "check-i18n-prod": "if [ -z \"${MINDS_LOCALE}\" ]; then { echo 'MINDS_LOCALE not set' && exit 1; }; fi",
    "precompile-i18n-prod": "npm run check-i18n-prod",
    "compile-i18n-prod": "npm run compile-aot -- --i18nFile=./app/locale/Minds.${MINDS_LOCALE}.${MINDS_FORMAT:-xlf} --locale=${MINDS_LOCALE} --i18nFormat=${MINDS_FORMAT:-xlf}",
    "prebundle-i18n-prod": "npm run check-i18n-prod",
    "bundle-i18n-prod": "npm run bundle -- -o public/js/build-aot.${MINDS_LOCALE}.${MINDS_RELEASE:-default}.js",
    "prebuild-index-i18n-prod": "npm run check-i18n-prod",
    "build-index-i18n-prod": "npm run build-index-prod -- --locale=${MINDS_LOCALE}"
  },
  "dependencies": {
    "@angular/animations": "~4.1.1",
    "@angular/common": "~4.1.1",
    "@angular/compiler": "~4.1.1",
    "@angular/compiler-cli": "~4.1.1",
    "@angular/core": "~4.1.1",
    "@angular/forms": "~4.1.1",
    "@angular/http": "~4.1.1",
    "@angular/platform-browser": "~4.1.1",
    "@angular/platform-browser-dynamic": "~4.1.1",
    "@angular/platform-server": "~4.1.1",
    "@angular/router": "~4.1.1",
    "angular-in-memory-web-api": "~0.3.0",
    "braintree-web": "2.15.2",
    "core-js": "^2.4.1",
    "es6-module-loader": "^0.17.8",
    "es6-promise": "^3.1.2",
    "glob": "^7.1.2",
    "gulp-rename": "^1.2.2",
    "intl": "^1.0.0",
    "jasmine-ts-async": "^1.0.0",
    "material-datetime-picker": "^2.4.0",
    "medium-editor": "^5.23.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "~5.0.1",
    "socket.io-client": "^1.4.5",
    "systemjs": "0.19.40",
    "tinymce": "^4.2.6",
    "zone.js": "~0.8.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "async": "^1.4.2",
    "babel": "^5.8.23",
    "chalk": "^1.1.1",
    "connect": "^3.3.5",
    "del": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.2",
    "gulp-bump": "^0.3.1",
    "gulp-concat": "^2.5.2",
    "gulp-css-globbing": "^0.1.4",
    "gulp-filter": "^2.0.2",
    "gulp-inject": "^1.3.1",
    "gulp-inline-ng2-template": "^1.0.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-minify-css": "^1.1.6",
    "gulp-minify-html": "^1.0.3",
    "gulp-plumber": "~1.0.1",
    "gulp-sass": "^2.0.3",
    "gulp-shell": "~0.4.3",
    "gulp-sourcemaps": "~1.5.2",
    "gulp-template": "^3.0.0",
    "gulp-tslint": "^3.3.0",
    "gulp-tslint-stylish": "^1.0.4",
    "gulp-typescript": "~2.13.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.2.4",
    "jasmine-core": "~2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-jasmine": "~0.3.6",
    "karma-typescript": "^3.0.4",
    "karma-typescript-angular2-transform": "^1.0.0",
    "merge-stream": "^1.0.0",
    "open": "0.0.5",
    "pixl-xml": "^1.0.10",
    "protractor": "^2.5.1",
    "rimraf": "^2.5.4",
    "rmv": "^1.0.0",
    "rollup": "^0.36.3",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "run-sequence": "^1.1.0",
    "sauce-connect-launcher": "^0.2.2",
    "semver": "^4.3.6",
    "serve-static": "^1.9.2",
    "slash": "~1.0.0",
    "stream-series": "^0.1.1",
    "systemjs-builder": "^0.14.8",
    "ts-node": "^3.0.2",
    "typescript": "~2.2.2",
    "typescript-register": "^1.1.0",
    "typings": "^1.3.0",
    "yargs": "^3.25.0"
  }
}
